{
  "htmlWhitespaceSensitivity": "strict",
  "semi": true,
  "arrowParens": "always",
  "bracketSpacing": true,
  "printWidth": 110,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "plugins": ["prettier-plugin-svelte"],
        "svelteSortOrder": "scripts-markup-styles"
      }
    }
  ]
}
